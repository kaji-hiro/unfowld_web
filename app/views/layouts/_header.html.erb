<header class='header'>
  <div class='header-link'>
    <%= image_pack_tag 'logo.png', class: 'header-link-logo' %>
    <% if logged_in? %>
      <%= link_to 'home', root_path, class: 'header-link-item' %>
      <%= link_to 'user', users_path, class: 'header-link-item' %>
      <%= link_to 'log_out', logout_path, method: :delete, class: 'header-link-item' %>
    <% else %>
      <%= link_to 'home', root_path, class: 'header-link-item' %>
      <%= link_to 'about', static_pages_about_path, class: 'header-link-item' %>
      <%= link_to 'contact', static_pages_contact_path, class: 'header-link-item' %>
      <%= link_to 'members', static_pages_members_path, class: 'header-link-item' %>
    <% end %>
  </div>
</header>
